<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE agent SYSTEM "../../../../config/dtd/agent.dtd">
<agent xmlns:proto="http://botlab.dev/protocol" dtd_version="1.0">
    <metadata>
        <name>Test Sequence Chain Agent</name>
        <type category="test">Test Type</type>
        <version>1.0</version>
        <timing>
            <response_interval unit="seconds">1</response_interval>
        </timing>
        <service>
            <provider>anthropic</provider>
            <model>claude-3-opus-20240229</model>
            <api_version>2024-02-15</api_version>
        </service>
    </metadata>

    <protocols>
        <protocol id="test_proto">
            <agent_definition>
                <objectives>
                    <primary>Execute sequence chain in order</primary>
                    <secondary>Maintain conversation context</secondary>
                    <secondary>Handle state transitions</secondary>
                </objectives>
                <style>
                    <communication>Clear and sequential</communication>
                    <analysis>Step-by-step progression</analysis>
                </style>
            </agent_definition>
        </protocol>
    </protocols>

    <momentum>
        <!-- Initialization sequence -->
        <sequence id="init" type="initialization" proto:ref="test_proto" temperature="0.7">
            <message position="1">
                <role type="system"/>
                <content>Begin initialization sequence</content>
            </message>
        </sequence>

        <!-- Greeting sequence -->
        <sequence id="greeting" type="interaction" proto:ref="test_proto" temperature="0.8">
            <message position="1">
                <role type="system"/>
                <content>Handle user greeting</content>
            </message>
        </sequence>

        <!-- Analysis sequence -->
        <sequence id="analysis" type="processing" proto:ref="test_proto" temperature="0.4">
            <message position="1">
                <role type="system"/>
                <content>Perform data analysis</content>
            </message>
        </sequence>

        <!-- Conclusion sequence -->
        <sequence id="conclusion" type="completion" proto:ref="test_proto" temperature="0.6">
            <message position="1">
                <role type="system"/>
                <content>Wrap up conversation</content>
            </message>
        </sequence>
    </momentum>
</agent> 